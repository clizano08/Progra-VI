@model IEnumerable<Infraestructure.Models.HistoricoDepreciacion>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Consecutivo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdActivo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Depreciacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaDepreciacion)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Consecutivo)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Activo.Descripcion)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Depreciacion)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.FechaDepreciacion)
    </td>


</tr>
}

</table>
